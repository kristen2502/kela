#include <iostream>
using namespace std;

class Person 
{
public:
    void personInfo() 
	{
        cout << "This is Person class." << endl;
    }
};

class Employee {
public:
    void employeeInfo() 
	{
        cout << "This is Employee class." << endl;
    }
};


class Manager : public Person, public Employee 
{
public:
    void managerInfo() 
	{
        cout << "This is Manager class." << endl;
    }
};

int main() 
{
    Manager m;

    m.personInfo();     
    m.employeeInfo();   
    m.managerInfo();  

    return 0;
}
