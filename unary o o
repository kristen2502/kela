#include <iostream>
using namespace std;
class Number {
    int x, y;
public:
    Number(int a, int b)
	{
        x = a;
        y = b;
    }
 
    void operator - () {
        x = -x;
        y = -y;
    }
    void display() {
        cout << "x = " << x << ", y = " << y << endl;
    }
};

int main() {
    Number n1(5, -10);

    cout << "Before overloading: ";
    n1.display();

    -n1; // Calls operator-()

    cout << "After overloading: ";
    n1.display();

    return 0;
}
